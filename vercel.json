{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1",
      "includeFiles": "{includes/**,pages/**,style.css}"
    }
  },
  "routes": [
    {
      "src": "/style.css",
      "dest": "/style.css"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
